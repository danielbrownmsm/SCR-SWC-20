<launch>
    <!-- Set up the bridge to the simulator -->
    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />

    <!-- Robot control nodes (Python2) -->
    <node pkg="swc_daniel" type="ControlNode.py" name="control_node" output="screen" required="true" />
    <node pkg="swc_daniel" type="SensorNode.py" name="sensor_node" output="screen" required="true" />
    <node pkg="swc_daniel" type="VisionNode.py" name="vision_node" output="screen" required="true" />
    <!-- <node pkg="swc_daniel" type="TimerNode.py" name="timer_node" output="screen" required="true" /> -->
</launch>